<h2>create a new address here</h2>

<%= form_for @address do |f| %>
  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.label :street %>
  <%= f.text_field :street %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :zipcode %>
  <%= f.number_field :zipcode %>

  <%= f.submit %>
<% end %>
